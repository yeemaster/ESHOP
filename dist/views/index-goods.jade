section.good
  a.good-content(href='#{_var_net}/hoteldetail?id=' + '#{goods[j]["objid"]}' + "&preid=" + '#{goods[j]["preobjid"]}')
    .img-wrapper
      .img-placeholder
        img(src='#{goods[j].img}')
        |                  
        span.tag.reservation
    |        
    .info-wrapper
      .title-line
        .title
          h3.item-name
            | #{goods[j].name}
        |                
        |                
        span.store #{goods[j].store}
      |             
      p.desc #{goods[j].desc}
      |       
      |         
      p.price-line
        ins #{goods[j].price}
        |             
        del.old-price #{goods[j]["old-price"]}
        |             
        i.privilege-icon
          span.text #{goods[j].privilege}
        |             
        span.bought #{goods[j].bought}
